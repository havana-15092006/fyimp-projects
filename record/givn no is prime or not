#include<stdio.h>
int main()
{
int n,i,flag=0;
printf("enter the number");
scanf("%d",&n);
if(n==1||n==0)
printf("the given number is not prime");
else


{for(i=2;i<n;i++)
{
if(n%i==0)
{flag=1;
break;
}
}
if(flag==1)
printf("the given number is not prime");
else
printf("the given number is prime");
}}