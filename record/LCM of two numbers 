include<stdio.h>
int LCM(int a,int b)
{
int max,i;

if (a>=b)
max=a;
else 
max= b;


for(i=max;;i++)
{
if(i%a==0 && i%b==0)
{break;}

}
return i ;
}

int main()
{
int num1,num2, lcm,gcd;
printf("enter two numbers");
scanf("%d%d",&num1,&num2);
lcm=LCM(num1,num2);
printf("\nLCM of %d and %d is %d",num1,num2,lcm);

}