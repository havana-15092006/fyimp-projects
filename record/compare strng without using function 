#include<stdio.h>
int main()
{
char str1[100],str2[100];
int i,flag;
printf("enter string1");
scanf("%s",str1);
printf("enter string2");
scanf("%s",str2);
for(i=0;str1[i]!='\0'&& str2[i]!='\0';i++)
{if (str1[i] != str2[i])
{flag=1;
}
}
if (flag==1)
printf("%s and %s are not same",str1,str2);
else
printf("%s and %s are same",str1,str2);
}